#include <stdio.h>

int main() {
    int number, sum;

    // Input the two-digit number
    scanf("%d", &number);

    // Calculate the sum of digits
    int tens = number / 10;
    int units = number % 10;
    sum = tens + units;

    // Output in the required format
    printf("Bird said:%d\n", number);
    printf("Alice must go in path-%d\n", sum);

    return 0;
}

