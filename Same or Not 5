#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
int n, i;
int even = 0, odd = 0;
scanf("%d", &n);
int arr[n]; 
for(i = 0; i < n; i++) {
    scanf("%d", &arr[i]);
    if(arr[i] % 2 == 0)
        even++;
    else
        odd++;
}
if(even == n)
    printf("Even\n");
else if(odd == n)
    printf("Odd\n");
else
    printf("Mixed\n");

return 0;
}
