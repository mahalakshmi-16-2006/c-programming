#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
int num, count = 0;
scanf("%d", &num);
if (num <= 0 || num > 10000000) {
    printf("Enter a Valid Input\n");
} else {  
    int temp = num;
    while (temp != 0) {
        temp /= 10;
        count++;
    }
    printf("The count of the given integer is: %d\n", count);
}
return 0;
}
