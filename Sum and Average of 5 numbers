#include <stdio.h>

int main() {
    int a, b, c, d, e;
    int sum;
    float average;

    // Input
    scanf("%d %d %d %d %d", &a, &b, &c, &d, &e);

    // Calculation
    sum = a + b + c + d + e;
    average = sum / 5.0;

    // Output
    printf("Sum of the 5 number is : %d\n", sum);
    printf("Average of the 5 number is : %.1f\n", average);

    return 0;
}






